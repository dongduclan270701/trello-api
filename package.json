{
  "name": "trello-api",
  "version": "1.0.0",
  "description": "APIs for trello clone app",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src",
    "clean": "del -f build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongduclan270701/trello-api.git"
  },
  "keywords": [],
  "author": "kassdev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dongduclan270701/trello-api/issues"
  },
  "homepage": "https://github.com/dongduclan270701/trello-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "eslint": "^8.13.0",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-plugin-react": "^7.29.4",
    "nodemon": "^2.0.15"
  }
}
